python run_tozs.py hjorth 6 >> mah_hjorth
python run_tozs.py hjorth 7 >> mah_hjorth
python run_tozs.py hjorth 8 >> mah_hjorth
python run_tozs.py hjorth 9 >> mah_hjorth
python run_tozs.py hjorth 10 >> mah_hjorth

