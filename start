python run_tozs.py csp 5 >> mah_csp
python run_tozs.py csp 6 >> mah_csp
python run_tozs.py csp 7 >> mah_csp
python run_tozs.py csp 8 >> mah_csp
python run_tozs.py csp 9 >> mah_csp
python run_tozs.py csp 10 >> mah_csp

